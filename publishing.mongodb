function createPublishers(){
    let springer = {name: "Springer", books: []}
    for (let i = 0; i < 10; i++){
        let book = {isbn: "ISBN"+i, title: "Title" + i, price: 0.99 * i}
        springer.books[i] = book
    }
    let addison = {name: "Addison", books: [{isbn: "ISBN42", title: "Title42", price: 42.99}]}
    db.publishing.insertOne(springer)
    db.publishing.insertOne(addison)

}
use ("training")
db.publishing.drop()
db.createCollection("publishing")
createPublishers()
